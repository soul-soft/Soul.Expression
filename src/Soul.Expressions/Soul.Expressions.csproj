<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
	  <TargetFramework>netstandard2.0</TargetFramework>
	  <Copyright>MIT</Copyright>
	  <PackageLicenseExpression>MIT</PackageLicenseExpression>
	  <RepositoryType>git</RepositoryType>
	  <RepositoryUrl>https://github.com/soul-soft/Soul.Expressions</RepositoryUrl>
	  <PackageProjectUrl>https://github.com/soul-soft/Soul.Expressions</PackageProjectUrl>
	  <IncludeSymbols>true</IncludeSymbols>
	  <EmbedUntrackedSources>true</EmbedUntrackedSources>
	  <SymbolPackageFormat>snupkg</SymbolPackageFormat>
	  <NoWarn>1701;1702;1591</NoWarn>
	  <Version>1.0.0</Version>
	  <PackageReleaseNotes>Preview Version</PackageReleaseNotes>
	  <PublishRepositoryUrl>true</PublishRepositoryUrl>
	  <Description>Expression</Description>
	  <Company>Soul</Company>
	  <Authors>Soul</Authors>
	  <PackageTags>Expression;Linq;Lambda;Eval;Compiler</PackageTags>
	  <PackageIcon>soul.png</PackageIcon>
  </PropertyGroup>
	<ItemGroup>
		<None Update="soul.png">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
	</ItemGroup>
</Project>
